  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-yRlYtNO2QJo-Rl3cqNgBwg" name="developer,_MP5EoPUBEeSTdOKYqDx6gQ" guid="-yRlYtNO2QJo-Rl3cqNgBwg">
